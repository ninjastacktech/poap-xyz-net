<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<RootNamespace>Poap</RootNamespace>
		<Description>.NET 6 C# SDK for poap.xyz</Description>
		<Authors>Cristi Pufu</Authors>
		<AssemblyName>Poap</AssemblyName>
		<PackageId>Poap</PackageId>
		<PackageTags>poap;httpclient;sdk;</PackageTags>
		<PackageProjectUrl>https://github.com/ninjastacktech/poap-xyz-net</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryType>git</RepositoryType>
		<RepositoryUrl>https://github.com/ninjastacktech/poap-xyz-net</RepositoryUrl>
		<Version>1.0.0</Version>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
	</ItemGroup>

</Project>
